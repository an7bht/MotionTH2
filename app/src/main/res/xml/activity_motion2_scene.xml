<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="cubic(0,2,0,2,0,8,0,8)">
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/imageView11"
                motion:framePosition="50"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.4"
                motion:percentY="0.33" />
            <KeyPosition
                motion:motionTarget="@+id/imageView12"
                motion:framePosition="73"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.5"
                motion:percentY="0.35" />
            <KeyPosition
                motion:motionTarget="@+id/imageView13"
                motion:framePosition="79"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.5"
                motion:percentY="0.33" />
        </KeyFrameSet>
        <OnClick motion:targetId="@+id/button" />

    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="104dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.501" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            android:layout_marginTop="88dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_marginTop="96dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView11"
            motion:layout_constraintEnd_toEndOf="@+id/button"
            motion:layout_constraintStart_toStartOf="@+id/button"
            motion:layout_constraintBottom_toTopOf="@+id/button"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.551" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/button2"
            android:layout_width="wrap_content"
            android:id="@+id/imageView12"
            motion:layout_constraintStart_toStartOf="@+id/button2"
            motion:layout_constraintBottom_toTopOf="@+id/button2"
            android:layout_marginBottom="12dp"
            motion:layout_constraintHorizontal_bias="0.488" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/button3"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintStart_toStartOf="@+id/button3"
            motion:layout_constraintBottom_toTopOf="@+id/button3"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.517" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView15"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button3"
            motion:layout_constraintVertical_bias="0.7" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView15"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button3"
            motion:layout_constraintVertical_bias="0.7" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="104dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/imageView15"
            android:layout_width="wrap_content"
            android:id="@+id/imageView11"
            motion:layout_constraintStart_toStartOf="@+id/imageView15"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/imageView15"
            android:alpha="0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/imageView15"
            android:layout_width="wrap_content"
            android:id="@+id/imageView12"
            motion:layout_constraintStart_toStartOf="@+id/imageView15"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView15"
            motion:layout_constraintTop_toTopOf="@+id/imageView15"
            android:alpha="0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/imageView15"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintStart_toStartOf="@+id/imageView15"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView15"
            motion:layout_constraintTop_toTopOf="@+id/imageView15" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/button2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            android:layout_marginTop="88dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/button3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_marginTop="96dp"
            motion:layout_constraintHorizontal_bias="0.498" />
    </ConstraintSet>
</MotionScene>